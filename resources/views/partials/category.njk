<ul class="nav">
  <li class="nav-item head">
    {{ category | capitalize | replace('-', ' ') }}
  </li>
  {% for child in childs %}
    <li class="nav-item {{ 'active' if meta.permalink == child.permalink else '' }}">
      <a class="nav-link" href="{{ child.permalink }}"> {{ child.title }} </a>
    </li>
  {% endfor %}
</ul>
